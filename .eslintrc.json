{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": ["warn"],
      "require-await": ["warn"],
      "no-const-assign": ["error"],
      "prefer-const": ["error"]
    }
}
